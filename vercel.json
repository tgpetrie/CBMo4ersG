{
  "version": 2,
  "builds": [
    { "src": "src/main.jsx", "use": "@vercel/static-build", "config": { "distDir": "dist" } },
    { "src": "api/**/*.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.py" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
